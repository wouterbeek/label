  rdfs:label
    "flertal"@da,
    "plural"@en,
    "plural"@es;
  rdfs:subPropertyOf rdfs:label.

label:singular
  a owl:AnnotationProperty;
  label:plural
    "entalsformer"@da,
    "formas singulares"@es,
    "singular forms"@en;
  label:singular
    "entalsform"@da,
    "forma singular"@es,
    "singular form"@en;
  rdfs:comment
    "A relation between a term and its label in literal singular form"@en,
